{
  "name": "nzws.me",
  "description": "nzws.me",
  "repository": "https://github.com/nzws/nzws.me.git",
  "author": "nzws <github@nzws.me>",
  "license": "MPL-2.0",
  "private": true,
  "engines": {
    "node": "12.x || 13.x || 14.x"
  },
  "scripts": {
    "lint:prettier": "prettier --check --ignore-path .gitignore .",
    "lint:js": "eslint --cache --ignore-path .gitignore \"**/*.{js,ts,tsx}\"",
    "lint:ts-next": "tsc",
    "lint:ts-node": "tsc --project ./tsconfig.node.json",
    "lint": "npm-run-all lint:* -s",
    "format-all": "prettier --write --ignore-path .gitignore .",
    "format:prettier": "pretty-quick",
    "format:js": "eslint --cache --ignore-path .gitignore --fix \"**/*.{js,ts,tsx}\"",
    "format": "npm-run-all format:* -s",
    "build": "yarn generate-index && next build",
    "generate-index": "ts-node --project ./tsconfig.node.json ./scripts/generate-index.ts",
    "dev": "next",
    "start": "next start",
    "analyze": "cross-env ANALYZE=true next build"
  },
  "devDependencies": {
    "@babel/core": "^7.10.3",
    "@next/bundle-analyzer": "^9.4.4",
    "@nzws/prettier-config": "~1",
    "@types/gtag.js": "^0.0.3",
    "@types/node": "^14.0.13",
    "@types/react": "^16.9.38",
    "@types/remove-markdown": "^0.1.1",
    "@typescript-eslint/eslint-plugin": "^3.3.0",
    "@typescript-eslint/parser": "^3.3.0",
    "@yuzulabo/eslint-config": "~1",
    "babel-eslint": "^10.0.3",
    "babel-plugin-styled-components": "^1.10.7",
    "cross-env": "^7.0.2",
    "eslint": "~7.1.0",
    "eslint-plugin-react": "^7.18.3",
    "npm-run-all": "~4",
    "prettier": "~2.0.5",
    "pretty-quick": "^2.0.1",
    "raw-loader": "^4.0.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5"
  },
  "dependencies": {
    "@icons-pack/react-simple-icons": "^3.1.1",
    "gray-matter": "^4.0.2",
    "next": "^9.4.4",
    "nextjs-progressbar": "^0.0.5",
    "polished": "^3.6.3",
    "preact": "^10.4.4",
    "preact-render-to-string": "^5.1.4",
    "react": "github:preact-compat/react#1.0.0",
    "react-dom": "github:preact-compat/react-dom#1.0.0",
    "react-feather": "^2.0.8",
    "react-ssr-prepass": "npm:preact-ssr-prepass@^1.0.1",
    "rehype-stringify": "^7.0.0",
    "remark-parse": "^8.0.2",
    "remark-rehype": "^7.0.0",
    "remove-markdown": "^0.3.0",
    "ress": "^2.0.4",
    "styled-components": "^5.1.1",
    "styled-media-query": "^2.1.2",
    "unified": "^9.0.0"
  },
  "prettier": "@nzws/prettier-config"
}
