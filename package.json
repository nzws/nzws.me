{
  "name": "nzws.me",
  "description": "nzws.me",
  "repository": "https://github.com/nzws/nzws.me.git",
  "author": "nzws <github@nzws.me>",
  "license": "MPL-2.0",
  "private": true,
  "engines": {
    "node": "12.x || 13.x || 14.x"
  },
  "scripts": {
    "lint:prettier": "prettier --check --ignore-path .gitignore .",
    "lint:js": "eslint --cache --ignore-path .gitignore \"**/*.js\"",
    "lint": "npm-run-all lint:* -s",
    "format-all": "prettier --write --ignore-path .gitignore .",
    "format:prettier": "pretty-quick",
    "format:js": "eslint --cache --ignore-path .gitignore --fix \"**/*.js\"",
    "format": "npm-run-all format:* -s",
    "build": "yarn generate-index && next build",
    "generate-index": "babel-node ./scripts/generate-index.js",
    "dev": "next",
    "start": "next start",
    "analyze": "cross-env ANALYZE=true next build"
  },
  "devDependencies": {
    "@babel/node": "^7.8.7",
    "@next/bundle-analyzer": "^9.4.2",
    "@nzws/prettier-config": "~1",
    "@yuzulabo/eslint-config": "~1",
    "babel-eslint": "^10.0.3",
    "babel-plugin-styled-components": "^1.10.7",
    "cross-env": "^7.0.2",
    "eslint": "~7.0.0",
    "eslint-plugin-react": "^7.18.3",
    "npm-run-all": "~4",
    "prettier": "~2.0.5",
    "pretty-quick": "^2.0.1",
    "raw-loader": "^4.0.1"
  },
  "dependencies": {
    "@icons-pack/react-simple-icons": "^3.0.0",
    "gray-matter": "^4.0.2",
    "next": "^9.4.1",
    "polished": "^3.6.3",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-feather": "^2.0.8",
    "rehype-stringify": "^7.0.0",
    "remark-parse": "^8.0.2",
    "remark-rehype": "^7.0.0",
    "remove-markdown": "^0.3.0",
    "ress": "^2.0.4",
    "styled-components": "^5.1.0",
    "styled-media-query": "^2.1.2",
    "unified": "^9.0.0"
  },
  "prettier": "@nzws/prettier-config"
}
